scout-ai=/$PWD filter="*.rb *.rake Rakefile *.rdoc *.R *.sh *.js *.haml *.sass *.txt *.conf" {
 Rakefile
 bin=bin filter="*"{
  scout-ai
 }
 etc=etc filter="*"{
  config
 }
 lib=lib {
  scout-ai.rb
  scout=scout{
   llm=llm{
    utils.rb
    parse.rb
    tools.rb
    chat.rb

    backends=backends{
     ollama.rb
     openai.rb
     openwebui.rb
     huggingface.rb
     relay.rb
    }

    ask.rb

    embed.rb

    rag.rb

    agent.rb
   }
  }
 }
 test=test {
  data=data filter="*"{
   person=person{
    identifiers
    brothers
    marriages
    parents
   }
  }
  test_helper.rb
 }
 data=data filter="*"{
 }
 scout_commands=scout_commands filter="*"{
  llm=llm{
   ask
   template
   process
  }
  agent=agent{
   ask
   kb
  }
 }
 questions=questions filter="*"{
  coach
  evaluator
  templater
 }
}
