scout-ai=$PWD filter="*.rb *.rake Rakefile *.rdoc *.R *.sh *.js *.haml *.sass *.txt *.conf" {
 Rakefile
 bin=bin filter="*"{
  scout-ai
 }
 lib=lib {
  scout-ai.rb
  scout=scout{
   llm=llm{
    utils.rb
    parse.rb
    tools.rb
    chat.rb

    backends=backends{
     openai.rb
     ollama.rb
     openwebui.rb
     huggingface.rb
     relay.rb
    }

    ask.rb

    embed.rb

    rag.rb

    agent.rb
    agent=agent{
     chat.rb
    }
   }
   model=model{
    util=util{
     save.rb
     run.rb
    }
    base.rb

    python=python{
     base.rb
     torch.rb
     torch=torch{
      helpers.rb
      dataloader.rb
      introspection.rb
      load_and_save.rb
     }
     huggingface.rb
     huggingface=huggingface{
      classification.rb
      causal.rb
     }
    }
   }
  }
 }
 python=python filter="*.py"{
  scout_ai=scout_ai{
   __init__.py
   language_model.py
   util.py
   huggingface=huggingface{
    data.py
    eval.py
    model.py
    train.py
   }
  }
 }
 chats=chats{
  test=test{
   imports.rb
  }
  agent=agent{
   chat
  }
  system=system{
   scout-ai
  }
  doc=doc{
   chat
   model.analysis
   model
  }
  develop=develop{
   chat
   model
   causalLM
  }
  refactor=refactor{
   chat
  }
 }

 test=test {
  data=data filter="*"{
   person=person{
    identifiers
    brothers
    marriages
    parents
   }
  }
  test_helper.rb
 }
 data=data filter="*"{
 }
 scout_commands=scout_commands filter="*"{
  llm=llm{
   ask
   template
   process
  }
  agent=agent{
   ask
   kb
  }
 }
}
