scout-ai=$PWD filter="*.rb *.rake Rakefile *.rdoc *.R *.sh *.js *.haml *.sass *.txt *.conf" {
 Rakefile
 README.md
 LICENSE.txt
 bin=bin filter="*"{
  scout-ai
 }
 doc=doc filter="*"{
  Agent.md
  Chat.md
  LLM.md
  Model.md
 }
 lib=lib {
  scout-ai.rb
  scout=scout{
   llm=llm{
    utils.rb
    parse.rb
    tools.rb
    tools=tools{
     workflow.rb
     knowledge_base.rb
     call.rb
    }
    chat.rb

    backends=backends{
     openai.rb
     responses.rb
     ollama.rb
     bedrock.rb
     openwebui.rb
     huggingface.rb
     relay.rb
    }

    ask.rb

    embed.rb

    rag.rb

    mcp.rb

    agent.rb
    agent=agent{
     chat.rb
     iterate.rb
    }
   }
   model=model{
    util=util{
     save.rb
     run.rb
    }
    base.rb

    python=python{
     base.rb
     torch.rb
     torch=torch{
      helpers.rb
      dataloader.rb
      introspection.rb
      load_and_save.rb
     }
     huggingface.rb
     huggingface=huggingface{
      classification.rb
      causal.rb
      causal=causal{
       next_token.rb
      }
     }
    }
   }
  }
 }
 python=python filter="*"{
  scout_ai=scout_ai{
   __init__.py
   language_model.py
   util.py
   huggingface=huggingface{
    data.py
    eval.py
    model.py
    train=train{
     __init__.py
     next_token.py
    }
    rlhf.py
   }
  }
 }
 test=test {
  data=data filter="*"{
   person=person{
    identifiers
    brothers
    marriages
    parents
   }
  }
  test_helper.rb
 }
 data=data filter="*"{
 }
 scout_commands=scout_commands filter="*"{
  documenter
  llm=llm{
   ask
   template
   process
   server
  }
  agent=agent{
   ask
   kb
  }
 }
 share=share{
  server=server files="*"{
   chat.html
   chat.js
  }
 }
}
