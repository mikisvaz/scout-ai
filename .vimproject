scout-ai=/$PWD filter="*.rb *.rake Rakefile *.rdoc *.R *.sh *.js *.haml *.sass *.txt *.conf" {
 Rakefile
 bin=bin filter="*"{
  scout-ai
 }
 lib=lib {
  scout-ai.rb
  scout=scout{
   llm=llm{
    parse.rb
    tools.rb
    backends=backends{
     ollama.rb
     openai.rb
     openwebui.rb
     relay.rb
    }
    ask.rb
    agent.rb
   }
  }
 }
 test=test {
  data=data filter="*"{
   person=person{
    brothers
    identifiers
    marriages
    parents
   }
  }
  test_helper.rb
 }
 data=data filter="*"{
 }
 scout_commands=scout_commands filter="*"{
  llm=llm{
   ask
   template
   process
  }
 }
 questions=questions filter="*"{
  coach
  evaluator
  templater
 }
}
