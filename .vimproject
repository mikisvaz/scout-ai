scout-ai=/$PWD filter="*.rb *.rake Rakefile *.rdoc *.R *.sh *.js *.haml *.sass *.txt *.conf" {
 Rakefile
 bin=bin filter="*"{
  scout-ai
 }
 lib=lib {
  scout-ai.rb
  scout=scout{
   llm=llm{
    parse.rb
    tools.rb
    openai.rb
    ollama.rb
    ask.rb
   }
  }
 }
 test=test {
  test_helper.rb
 }
 data=data filter="*"{
 }
 scout_commands=scout_commands filter="*"{
  llm=llm{
   ask
   template
  }
 }
 questions=questions filter="*"{
  coach
  evaluator
  templater
 }
}
